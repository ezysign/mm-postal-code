      "module": "commonjs",
      "target": "es2015",
      "declaration": true,
      "isolatedModules": true,
      "outDir": "./dist",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "typeRoots": ["types","node_modules/@types"],
