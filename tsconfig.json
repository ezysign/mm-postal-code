{
    "compilerOptions": {
      "module": "commonjs",
      "target": "es2015",
      "declaration": true,
      "isolatedModules": true,
      "outDir": "./dist",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "typeRoots": ["types","node_modules/@types"],
      "sourceMap": false,
      "lib": [
        "es5", "es6", "dom", "dom.iterable"
      ]
    },
    "include": ["./src/**/*.ts"],
    "exclude":["./**/*.test.ts","./example"]
    
  }